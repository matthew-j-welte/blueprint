{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "ExerciseService": {
    "ExerciseApprovalLockTimeoutSeconds": 3600
  },
  "DatabaseInfo": {
    "DatabaseName": "exercise-db",
    "DatabaseContainers": {
      "ExerciseContainer": {
        "Name": "exercise-container"
      }
    }
  }
}
